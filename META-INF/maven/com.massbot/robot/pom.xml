    <finalName>robot</finalName>
    
    <plugins>
		<plugin>
			<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
		</plugin>
			
		<plugin>  
       		<groupId>org.apache.maven.plugins</groupId>  
       		<artifactId>maven-surefire-plugin</artifactId>  
       		<configuration>  
         		<skipTests>true</skipTests>  
       		</configuration>  
     	</plugin>  
	</plugins>
